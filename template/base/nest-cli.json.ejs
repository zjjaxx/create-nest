{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "plugins": [
    <%_ for (const plugin of plugins) { _%>
    <%- plugin %><% if (plugins.indexOf(plugin) !== plugins.length - 1) { %>,<% } _%>
    <%_ } %>
    ],
    "watchAssets": <%- watchAssets _%> ,
    "assets": [
    <%_ for (const asset of assets) { _%>
    <%- asset %><% if (assets.indexOf(asset)!== assets.length - 1) { %>,<% } _%>
    <%_ } %>
    ]
  }
}
